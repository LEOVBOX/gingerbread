<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hBox" minHeight="60.0" prefHeight="93.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gingerbread.ResourceController">
   <children>
      <Label fx:id="label" prefHeight="107.0" prefWidth="414.0" style="-fx-border-width: 4; -fx-background-color: transparent; -fx-border-color: #d4d5d6; -fx-border-radius: 6;" />
      <Button fx:id="removeButton" contentDisplay="CENTER" maxWidth="60" mnemonicParsing="false" onAction="#removeResource" prefHeight="104.0" prefWidth="187.0">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@close.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</HBox>
